{
    "name": "sm-alfariz/lumen52-with-jwt",
    "description": "Lumen micro-framework by Laravel with jwt integration ready and free for use ",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.2.*",
        "vlucas/phpdotenv": "~2.2",
        "tymon/jwt-auth": "1.0.*@dev",
        "illuminate/routing": "^5.2"        
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
	    "files": [
	      "app/helpers.php"
	    ]        
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    }
}
